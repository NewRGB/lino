<MenuButton
        class="lino_MenuButton"
        buttonMode="Split"
        defaultAction="onPressAction"
        {% if ba.action.ui5_icon_name -%}
        icon="{{ba.action.ui5_icon_name}}"
        {%- else -%}
        text="{{ba.action.label}}"
        {% endif %}
        tooltip="{{ba.action.help_text}}"
        customData:action_url="{{actor.app_label}}/{{actor.__name__}}/">
    <customData>
        <core:CustomData
                key="action_name"
                value="{{ba.action.action_name}}"/>
        <core:CustomData
                key="action_method"
                value="{{ba.action.http_method}}"/>
    </customData>
<menu>
<Menu itemSelected="UNUSED_onMenuAction">
